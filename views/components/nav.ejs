<header class="masthead mb-auto">
    <div class="inner">
        <h3 class="masthead-brand">Fox Award</h3>
        <nav class="nav nav-masthead justify-content-center" id="navigation">
        <a class="nav-link <% if(!where){ %>active<% } %>" href="/">Home</a>
        <% if(!logged) { %>
            <a class="nav-link <% if(where === 'register'){ %>active<% } %>" href="/voter/register">Register</a>
            <a class="nav-link <% if(where === 'login'){ %>active<% } %>" href="/voter/login">Login</a>
            <% } else {  %>
            <a class="nav-link <% if(where === 'movies'){ %>active<% } %>" href="/movies">Movies</a>
            <a class="nav-link <% if(where === 'voter'){ %>active<% } %>" href="/voter/<%= user.id %>/edit"><%=user.name%></a>
            <div style="width:20px; height:20px"><img src="/<%=user.path%>" class="img-fluid ml-3" alt=""></div>
        <% } %>
        </nav>
    </div>
</header>