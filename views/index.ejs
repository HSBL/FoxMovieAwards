<%- include('./partial/head') %>
<%- include('./components/nav', {where: null, logged, user}) %>
<div class="content-center">
    <h1>Welcome, Current Standing:</h1>
    <div class="table-responsive">
        <table class="table table-striped table-dark">
            <thead>
            <tr>
                <th>Nomination</th>
                <th>Movie Title</th>
                <th>Total Votes</th>
            </tr>
            </thead>
            <tbody>
                <% for (let category in result[0]) {%>
                    <tr>
                        <td><%= category %></td>
                        <td><%= result[0][category].winner || 0 %></td>
                        <td><%= result[0][category].Count || 0 %></td>
                    </tr>
                <% } %>
            </tbody>
        </table>
    </div>
</div>
<%- include('./partial/foot') %>